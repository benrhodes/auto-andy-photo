<g xmlns="http://www.w3.org/2000/svg">
	<rect class="selection-rect" x="0" y="0" fill="none" stroke="{{strokeColor}}" stroke-width="{{strokeSize}}" stroke-miterlimit="10" width="200" height="200"/>
	<rect class="ul-scale-handle" x="-15" y="-15" fill="{{fillColor}}" stroke="{{strokeColor}}" stroke-width="{{strokeSize}}" stroke-miterlimit="10" width="{{handleSize}}" height="{{handleSize}}"/>
	<rect class="ur-scale-handle" x="185" y="-15" fill="{{fillColor}}" stroke="{{strokeColor}}" stroke-width="{{strokeSize}}" stroke-miterlimit="10" width="{{handleSize}}" height="{{handleSize}}"/>
	<rect class="ll-scale-handle" x="-15" y="185" fill="{{fillColor}}" stroke="{{strokeColor}}" stroke-width="{{strokeSize}}" stroke-miterlimit="10" width="{{handleSize}}" height="{{handleSize}}"/>
	<rect class="lr-scale-handle" x="185" y="185" fill="{{fillColor}}" stroke="{{strokeColor}}" stroke-width="{{strokeSize}}" stroke-miterlimit="10" width="{{handleSize}}" height="{{handleSize}}"/>
	<g class="rotation-group" transform="translate(100, 0)">
		<circle class="rotate-handle" fill="{{fillColor}}" stroke="{{strokeColor}}" stroke-width="{{strokeSize}}" stroke-miterlimit="10" cx="0" cy="{{rotationHandleYOffset}}" r="{{rotationHandleRadius}}"/>
		<line fill="none" stroke="{{strokeColor}}" stroke-width="{{strokeSize}}" stroke-miterlimit="10" x1="0" y1="-40" x2="0" y2="0"/>
	</g>
</g>